{
  "expo": {
    "name": "Union",
    "slug": "unionapp",
    "scheme": "unionapp",
    "description": "Union app",
    "privacy": "unlisted",
    "platforms": [
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#5CCCA4"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.draftbit.untitledapp",
      "supportsTablet": false,
      "buildNumber": "1.0.0",
      "infoPlist": {},
      "usesIcloudStorage": false
    },
    "android": {
      "versionCode": 1,
      "package": "fr.unionfit.union",
      "permissions": [
        "CAMERA"
      ]
    },
    "web": {
      "themeColor": "#000000"
    },
    "notification": {
      "androidMode": "default"
    }
  }
}